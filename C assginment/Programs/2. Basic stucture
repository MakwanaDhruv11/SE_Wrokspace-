// Write a C program that includes variables, constants, and comments. Declare
and use different data types (int, char, float) and display their values.

#include<stdio.h>
#include<conio.h>
int main()
{
    int i=10;
    float f=99.75;
    char c='v';

    // comments

    printf(" int = %d",i);
    printf(" float = %f",f);
    printf(" char = %c",c);


    return 0;
}
